{
    "name": "Screen Share",
    "description": "Screen Sharing",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "hello_extensions.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "desktopCapture",
        "tabCapture",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    }
}